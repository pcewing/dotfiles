# gitconfig
[include]
    path = ~/.gitconfig_local
[alias]
    s = status --short
    ss  = status
    co = checkout
    cob = checkout -b
    br = branch -v
    cont = rebase --continue

    # current branch
    cbr = rev-parse --abbrev-ref HEAD

    diff = diff --ignore-space-at-eol -b -w --ignore-blank-lines
